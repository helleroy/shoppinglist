{
  "name": "handleliste",
  "version": "0.1.0",
  "private": true,
  "repository": "https://github.com/helleroy/shoppinglist",
  "dependencies": {
    "firebase": "^5.5.3",
    "lodash": "^4.17.11",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
